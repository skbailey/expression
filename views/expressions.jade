extends layout

block page-styles
  link(rel="stylesheet", href="/stylesheets/expressions.css", media="all")

block content
  .container.all
    .row
      .col-md-6
        h3 French
      .col-md-6
        h3 English

    each expression in expressions
      .row
        .col-md-6
          = expression.french
        .col-md-6
          = expression.english